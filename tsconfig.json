{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "rootDir": ".",
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./src/*"],
      "@list/core": ["./src/domains/list/core/src/index"],
      "@list/core/*": ["./src/domains/list/core/src/*"],
      "@list/hook": ["./src/domains/list/hook/src/index"],
      "@list/hook/*": ["./src/domains/list/hook/src/*"]
    }
  },
  "include": ["./src"]
}
